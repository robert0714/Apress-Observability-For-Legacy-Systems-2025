				"body": {
					"mode": "raw",
					"raw": "{\n  \"allowGuaranteedMsgReceiveEnabled\": true,\n  \"allowGuaranteedMsgSendEnabled\": true,\n  \"rejectMsgToSenderOnNoSubscriptionMatchEnabled\": true\n}"
